diff -rupN binutils-2.20-orig/gas/configure.tgt binutils-2.20/gas/configure.tgt
--- binutils-2.20-orig/gas/configure.tgt	2010-02-04 09:21:45.000000000 +0100
+++ binutils-2.20/gas/configure.tgt	2010-02-04 09:26:07.298465633 +0100
@@ -96,6 +96,7 @@ esac
 generic_target=${cpu_type}-$vendor-$os
 # Note: This table is alpha-sorted, please try to keep it that way.
 case ${generic_target} in
+  i386-*-tylyos*)                       fmt=elf ;;
   alpha-*-*vms*)			fmt=evax ;;
   alpha-*-osf*)				fmt=ecoff ;;
   alpha-*-linuxecoff*)			fmt=ecoff ;;
diff -rupN binutils-2.20-orig/ld/configure.tgt binutils-2.20/ld/configure.tgt
--- binutils-2.20-orig/ld/configure.tgt	2010-02-04 09:21:44.000000000 +0100
+++ binutils-2.20/ld/configure.tgt	2010-02-04 09:27:20.994436842 +0100
@@ -27,6 +27,7 @@ targ64_extra_libpath=
 # architecture variants should be kept together even if their names
 # break the alpha sorting.
 case "${targ}" in
+i[3-7]86-*-myos*)       targ_emul=tylyos_i386 ;;
 alpha*-*-freebsd* | alpha*-*-kfreebsd*-gnu)
 		        targ_emul=elf64alpha_fbsd
 			targ_extra_emuls="elf64alpha alpha"
